<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>StudentRegistration</display-name>
  
  <servlet>
    <servlet-name>StudentDetails</servlet-name>
    <servlet-class>
    	Student.StudentDetails
    </servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>StudentDetails</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>WelcomeDetails</servlet-name>
    <servlet-class>
    	Student.WelcomeStudnetDetails
    </servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>WelcomeDetails</servlet-name>
    <url-pattern>/WelcomeStudnetDetails</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>Design/studentRegistration.html</welcome-file>
  </welcome-file-list>
</web-app>